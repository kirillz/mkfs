{
  "name": "mkfsapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn build:css && yarn start:docker && nodemon ./bin/www",
    "build:css": "npx tailwindcss build ./public/stylesheets/source.css -o ./public/stylesheets/style.css",
    "start:docker": "docker-compose up > /dev/null 2>&1 &",
    "stop:docker": "docker-compose down",
    "dev": "yarn build:css && yarn start:docker && nodemon ./bin/www"
  },
  "dependencies": {
    "alpinejs": "^2.7.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "^4.16.4",
    "http-errors": "~1.6.3",
    "mongod": "^2.0.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.7",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "nodemon": "^2.0.4",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.0.0",
    "tailwindcss": "^1.8.10"
  }
}
